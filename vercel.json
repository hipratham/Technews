{
    "version": 2,
    "builds": [
        {
            "src": "technews_project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "technews_project/wsgi.py"
        }
    ],
    "crons": [
        {
            "path": "/generate-news",
            "schedule": "0 3 * * *"
        }
    ]
}
